    "typescript": "^3.2.1",
    "uglify-js": "^3.4.9"
  },
  "peerDependencies": {
    "@types/react": "^16.8.3",
    "@types/react-dom": "^16.8.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2"
  },
  "dependencies": {
    "@types/react": "^16.8.6",
